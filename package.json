{
  "name": "carols-blog",
  "author": "Carolina Gilabert <hello@carolgilabert.me>",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.12.2",
    "gatsby": "^2.20.30",
    "gatsby-image": "^2.3.5",
    "gatsby-plugin-catch-links": "^2.2.4",
    "gatsby-plugin-fathom": "^1.2.0",
    "gatsby-plugin-feed": "^2.4.4",
    "gatsby-plugin-manifest": "^2.3.7",
    "gatsby-plugin-offline": "^3.1.5",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.5.7",
    "gatsby-plugin-styled-components": "^3.2.4",
    "gatsby-plugin-twitter": "^2.2.5",
    "gatsby-plugin-webmention": "^0.2.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.2.6",
    "gatsby-source-filesystem": "^2.2.5",
    "gatsby-transformer-json": "^2.3.4",
    "gatsby-transformer-remark": "^2.7.5",
    "gatsby-transformer-sharp": "^2.4.7",
    "parse-filepath": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0"
  },
  "keywords": [
    "gatsby",
    "blog"
  ],
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^25.4.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.3.5",
    "cypress": "^4.4.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "styled-components": "^5.1.0",
    "wait-on": "^4.0.2"
  },
  "jest": {
    "setupFiles": [],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "testRegex": "(\\.(test|spec))\\.(jsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/",
      "/public/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/.cache/**",
      "!**/public/**"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
